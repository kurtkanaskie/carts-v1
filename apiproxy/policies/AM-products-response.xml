<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-products-response">
    <DisplayName>AM-products-response</DisplayName>
    <Set>
        <Payload contentType="application/json">
{ 
    "Cart" : {cart}, 
    "Order" : { 
        "StoreID" : "{storeId}",
        "Products" : {products}
    }
}
</Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>